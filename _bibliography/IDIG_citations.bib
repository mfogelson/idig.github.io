	author = {Cagan, Jonathan and Campbell, Matthew I. and Finger, Susan and Tomiyama, Tetsuo},
	month = sep,
	year = {2005},
	pages = {171--181},
	pdf = {pdfs/Cagan et al., A Framework for Computational Design Synthesis- Model and Applications, JCEIS 2005.pdf}
}

